@model ChildrenBookStore.Model.ChildBookDetail
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

    <h4>ChildBookDetail</h4>
    <hr />
